{
    "manifest_version": 3,
    "name": "BROWSER EXTENSION",
    "description": "get audio results",
    "version": "2.2.0",
    "author": "@shevabam",
    "homepage_url": "https://github.com/wazeer1/search_extension.git",
    "permissions": [
        "scripting",
        "activeTab",
        "notifications",
        "storage", 
        "tabs", 
        "https://www.googleapis.com/"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "action": {
        "default_icon" : "img/R.png",
        "default_popup": "popup.html",
        "default_title": "search for audio"
    },
    "background": {
        "service_worker": "js/background.js"
    },
    "icons": {
        "128" : "img/R.png"
    }
}